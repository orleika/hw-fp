{
  "name": "rSMBIOS",
  "version": "0.1.0",
  "description": "showing smbios on a electron app and reporting it to the research page",
  "main": "dist/main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run compile && electron-packager . rSMBIOS --overwrite --platform=darwin,win32 --arch=x64 --asar --overwrite --ignore=\"^(build|node_modules|src|.babelrc|.gitignore|config.yml*|package*|webpack*)\" --asar.unpack=**/bin/** --out=./build",
    "compile": "webpack --progress",
    "lint": "standard src/**/*.js",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "orleika <admin@orleika.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-html": "^1.0.6",
    "gulp-riot": "^1.1.1",
    "gulp-sass": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "riotjs-loader": "^4.0.0",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "webpack": "^3.8.1",
    "yml": "^1.0.0"
  },
  "dependencies": {
    "material-components-web": "^0.25.0",
    "material-design-icons": "^3.0.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "riot": "^3.7.4"
  },
  "standard": {
    "globals": [
      "APP_NAME",
      "APP_VERSION",
      "REPORT_URL",
      "THANKS_URL"
    ]
  }
}
